# Copy to .env and fill in (do not commit .env).
# For Angular, copy these into src/environments/environment.ts and environment.prod.ts.

# Firebase (from Firebase Console > Project settings > Your apps)
FIREBASE_API_KEY=
FIREBASE_AUTH_DOMAIN=
FIREBASE_PROJECT_ID=
FIREBASE_STORAGE_BUCKET=
FIREBASE_MESSAGING_SENDER_ID=
FIREBASE_APP_ID=

# Google Maps JavaScript API (from Google Cloud Console)
GOOGLE_MAPS_API_KEY=

# For Cloud Functions (set via Firebase config or Secret Manager)
# GEMINI_API_KEY=  # Google AI Studio or Vertex AI API key
